<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.groupc.officelocator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.groupc.officelocator.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.groupc.officelocator.campus" />
        <activity android:name="com.groupc.officelocator.floorplan" />
        <activity android:name="com.groupc.officelocator.directions" />
        <activity android:name="com.groupc.officelocator.splash">
        </activity>
    </application>

</manifest>